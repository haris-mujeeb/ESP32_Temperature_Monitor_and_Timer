void setup() {
    display.init(SCREEN_HEIGHT, SCREEN_WIDTH);
    display.setRotation(1);
    display.fillScreen(ST77XX_BLACK);  // Clear the screen
}

int frame = 0;
const int BIT_MAP_X = 48;
const int BIT_MAP_Y = 16;

const byte (*pFrames)[288];

void loop() {
  pFrames = thermometer_icon_frames;
  for(int i = 0; i < 50; i++) {
    display.fillRect(BIT_MAP_X, BIT_MAP_Y, FRAME_WIDTH, FRAME_HEIGHT, ST77XX_BLACK);  // Clear the screen
    display.drawBitmap(48, 16, pFrames[frame], FRAME_WIDTH, FRAME_HEIGHT, ST77XX_WHITE);
    frame = (frame + 1) % FRAME_COUNT;
    delay(FRAME_DELAY);
  }
  pFrames = fire_icon_frames;
  for(int i = 0; i < 50; i++) {
    display.fillRect(BIT_MAP_X, BIT_MAP_Y, FRAME_WIDTH, FRAME_HEIGHT, ST77XX_BLACK);  // Clear the screen
    display.drawBitmap(48, 16, pFrames[frame], FRAME_WIDTH, FRAME_HEIGHT, ST77XX_ORANGE);
    frame = (frame + 1) % FRAME_COUNT;
    delay(FRAME_DELAY);
  }
}